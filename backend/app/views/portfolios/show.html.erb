<main class="container pt-5">
  <h4 class="mb-3">Show Portfolio</h4>
  <div class="row g-3">
    <div class="col-md-6">
      <small>Title</small>
      <div class="fs-4"><%= portfolio.title %></div>
    </div>
    <div class="col-md-6">
      <small>Url</small>
      <div class="fs-4"><%= portfolio.url %></div>
    </div>
    <div class="col-md-3">
      <small>Notion key</small>
      <div class="fs-4"><%= portfolio.notion_key %></div>
    </div>
    <div class="col-12">
      <small>Summary</small>
      <div class="fs-5"><%= portfolio.summary %></div>
    </div>
  </div>
</main>
